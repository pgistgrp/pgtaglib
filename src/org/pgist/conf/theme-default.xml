<?xml version='1.0' encoding='UTF-8'?>
<theme name="default">


    <listTable>
        <table>
            <property name="width"       value="100%"/>
            <property name="rules"       value="all"/>
            <property name="bgcolor"     value="#C5C5C3"/>
            <property name="frame"       value="box"/>
            <property name="cellspacing" value="1"/>
            <property name="cellpadding" value="2"/>
            <property name="border"      value="1"/>
            <property name="style"       value="border-collapse:collapse; border:solid 2px #8CACBB;"/>
        </table>
        <caption>
            <property name="color"   value=""/>
            <property name="bgcolor" value=""/>
        </caption>
        <header>
            <property name="color"   value=""/>
            <property name="bgcolor" value=""/>
        </header>
        <row>
            <alter-color value="white"/>
            <alter-color value="#DEE7EC"/>
            <highlight  value="#AFD671"/>
            <property name="style"       value="background-color:${background-color}"/>
            <property name="onmouseover" value="this.style.backgroundColor='${hightlight}'"/>
            <property name="onmouseout"  value="this.style.backgroundColor='${background-color}'"/>
        </row>
        <footer>
        </footer>
        <scroller>
        </scroller>
    </listTable>
    
    
    <scroller>
        <image>
            <property name="pgi" value="/images/pgi.gif" />
            <property name="s"  value="/images/s.gif" />
            <property name="t" value="/images/t.gif" />
        </image>
    </scroller>
    
    
</theme>

